{
	"presets": [
		"es2015", 
		"react"
	],
	"plugins": [
		"add-module-exports",
 		["transform-runtime", {
 			"helpers": true,
 		    "polyfill": false,
 		    "regenerator": true,
 		    "moduleName": "babel-runtime"
 		}],
 		"transform-async-to-generator",
 		"transform-object-rest-spread"
	]
}